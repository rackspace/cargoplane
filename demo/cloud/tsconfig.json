{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2020",
      "lib": ["es2022"],
      "declaration": true,
      "strict": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "esModuleInterop": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "outDir": ".build",
      "rootDir": "./"
    }
}